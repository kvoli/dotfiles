<frescobaldi-theme name="#Void Bear">
  <!--
  Created by base16-frescobaldi
  Author: #kvoli (https://github.com/kvoli)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#b2c1aa" />
    <background color="#000000" />
    <selectiontext color="#AA5B2B" />
    <selectionbackground color="#7c8776" />
    <current color="#655B53" />
    <mark color="#6F665B" />
    <error color="#655B53" />
    <search color="#" />
    <match color="#776E61" />
    <paper color="#655B53" />
    <musichighlight color="#7c8776" />
  </baseColors>
  <defaultStyles>
    <string textColor="#524B46" />
    <escape textColor="#524B46" />
    <comment italic="True" textColor="#7c8776" />
    <value textColor="#6F665B" />
    <variable textColor="#655B53" />
    <keyword bold="True" />
    <function bold="True" textColor="#776E61" />
    <error textColor="#AA5B2B" underline="True" underlineColor="#AA5B2B" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#524B46" />
      <dynamic bold="True" textColor="#776E61" />
      <articulation bold="True" textColor="#524B46" />
      <fingering textColor="#655B53" />
      <stringnumber textColor="#524B46" />
      <slur bold="True" />
      <markup bold="False" textColor="#6F665B" />
      <lyricmode textColor="#776E61" />
      <lyrictext textColor="#b2c1aa" />
      <context bold="True" />
      <grob textColor="#b2c1aa" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
